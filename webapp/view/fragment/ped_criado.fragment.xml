<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Pedido(s) criado com sucesso">
		<Table noDataText="No Products Found" items="{path:'/PedCriado'}" mode="MultiSelect" width="auto">
			<columns>
				<Column hAlign="Center" width="10em">
					<header>
						<Label text="Pedido"/>
					</header>
				</Column>
				<Column hAlign="Center" width="10em">
					<header>
						<Label text="Loja"/>
					</header>
				</Column>
			</columns>
			<items>
				<ColumnListItem type="Active">
					<cells>
						<Text text="{PedEbeln}"/>
						<Text text="{Werks}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
		<buttons>
			<Button text="Imprime" press="_handlePedCriadoPrint" icon="sap-icon://print"/>
			<Button text="e-Mail" press="_handlePedCriadoEmail" icon="sap-icon://email" visible="false"/>
			<Button text="Fecha" press="_handlePedCriadoClose" type="Reject"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>